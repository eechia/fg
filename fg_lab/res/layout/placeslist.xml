<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >
	<Button
        android:id="@+id/places_backBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        
         
        />
    
	

    <ListView
        android:id="@android:id/list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/homeBtn"
        android:layout_below="@+id/places_backBtn"
        android:layout_alignParentLeft="true"
   >

    </ListView>

    <Button
        android:id="@+id/addPlacesBtn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/feedgeorgeLogo"
        android:text="Add new place" />
   
    <TextView
        android:id="@+id/feedgeorgeLogo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/places_backBtn"
        android:text="feedgeorge" />

</RelativeLayout>